load fisheriris.mat;

label = zeros(150,3);

for i = 1 : 150
    if i <=50
        label(i,1) = 1;
    elseif i <=100
        label(i,2) = 1;
    else 
        label(i,3) = 1;
    end
end

iris = [meas label];

for i = 1:4,
    max = max(iris(:, i));
    min = min(iris(:, i));
    for j = 1:150,
        iris(j,i) = (iris(j,i)-min)/(max-min);
    end
end
%[trainind, ~, testind] = dividerand(iris', .8, 0, .2);